<?xml version="1.0" encoding="UTF-8"?>
<ScenarioController OutputDirectory="root_installs/AssetData/SATC_Tracks/Output"
	xmlns="http://www.mwc.org/asset">
	<ScenarioGenerator></ScenarioGenerator>

	<ObserverList>
		<TimeObserver Name="Domesday Timer" Active="true">
			<Duration Value="120" Units="minutes"></Duration>
		</TimeObserver>
		<DebriefReplayObserver Name="Freq_RedTrack"
			record_detections="true" record_positions="true" Active="true"
			record_decisions="false" file_name="Freq_RedTrack">
			<SubjectToTrack>
				<Type Name="RED" />
			</SubjectToTrack>
		</DebriefReplayObserver>
		<DebriefReplayObserver Name="Freq_BlueSensor"
			record_detections="true" record_positions="false" Active="true"
			record_decisions="false" file_name="Freq_BlueSensor">
			<SubjectToTrack>
				<Type Name="BLUE" />
			</SubjectToTrack>
		</DebriefReplayObserver>
		<DebriefReplayObserver Name="Freq_BlueCuts"
			record_detections="false" record_positions="true" Active="true"
			record_decisions="false" file_name="Freq_BlueTrack">
			<SubjectToTrack>
				<Type Name="BLUE" />
			</SubjectToTrack>
		</DebriefReplayObserver>
		<CSVTrackObserver Name="HiFreq_RedTrack"
			record_detections="true" record_positions="true" Active="true"
			record_decisions="false" file_name="Scen3_RedTrack">
			<SubjectToTrack>
				<Type Name="RED" />
			</SubjectToTrack>
		</CSVTrackObserver>
		<CSVTrackObserver Name="HiFreq_BlueSensor"
			record_detections="true" record_positions="false" Active="true"
			record_decisions="false" file_name="Scen3_BlueSensor">
			<SubjectToTrack>
				<Type Name="BLUE" />
			</SubjectToTrack>
		</CSVTrackObserver>
		<CSVTrackObserver Name="HiFreq_BlueCuts"
			record_detections="false" record_positions="true" Active="true"
			record_decisions="false" file_name="Scen3_BlueTrack">
			<SubjectToTrack>
				<Type Name="BLUE" />
			</SubjectToTrack>
		</CSVTrackObserver>
		<TrackPlotObserver Active="true" Name="Scen3_TrackPlot"
			append_dtg="false">
			<GridDelta Value="1" Units="km" />
		</TrackPlotObserver>
	</ObserverList>

</ScenarioController>
